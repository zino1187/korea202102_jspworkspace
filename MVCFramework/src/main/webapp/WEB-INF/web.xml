<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">

	
	<servlet>
		<servlet-name>bloodController</servlet-name>
		<servlet-class>com.koreait.mvcframework.controller.blood.BloodController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>bloodController</servlet-name>
		<url-pattern>/blood</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>movieController</servlet-name>
		<servlet-class>com.koreait.mvcframework.controller.movie.MovieController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>movieController</servlet-name>
		<url-pattern>/movie</url-pattern>
	</servlet-mapping>
	
</web-app>